<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="ehcache.xsd">
    <cacheManagerEventListenerFactory class="" properties=""/>
    <defaultCache 
       maxElementsInMemory="10000"
       eternal="false"
       timeToIdleSeconds="120"
       timeToLiveSeconds="120"
       memoryStoreEvictionPolicy="LRU">
       <persistence strategy="none"/>
    </defaultCache>

    <cache name="widgetCache"  maxElementsInMemory="51200" eternal="false"  timeToIdleSeconds="0" timeToLiveSeconds="30" />
    <cache name="appCache" maxElementsInMemory="25600" eternal="false" />
    <cache name="themeUriCache" maxElementsInMemory="25600" eternal="false" />
    <cache name="eopDNS" maxElementsInMemory="25600" eternal="false" />
    <cache name="regionCache" maxElementsInMemory="25600" eternal="false" />
    <cache name="siteMenuList" maxElementsInMemory="25600" eternal="false" />
    <cache name="goods_cat" maxElementsInMemory="25600" eternal="false" />
    <cache name="article_cat" maxElementsInMemory="25600" eternal="false" />
    <cache name="sitemap" maxElementsInMemory="25600" eternal="false" />
</ehcache>